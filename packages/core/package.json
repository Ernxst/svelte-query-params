			},
			"require": {
				"types": "./dist/adapters/dom/index.d.cts",
				"import": "./dist/adapters/dom/index.cjs"
			}
		}
	},
	"main": "dist/index.svelte.cjs",
	"module": "dist/index.svelte.js",
	"types": "dist/index.svelte.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=v18.12.1"
	},
	"scripts": {
		"build": "tsup",
		"postbuild": "publint",
		"-------------------------------------------------------------------------------": "",
		"typecheck": "tsc",
		"--------------------------------------------------------------------------------": "",
		"test": "bun test"
